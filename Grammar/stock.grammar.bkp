<prog> ::= (<buyrule> <sellrule> <shortrule> <coverrule>)
<buyrule> ::= (buy <node>)
<sellrule> ::= (sell <node>)
<shortrule> ::= (short <node>)
<coverrule> ::= (cover <node>)
<node> ::= (<boolean_operator> <pbf> <pbf>)
<boolean_operator>: (and) | (or)
<pbf> ::= (close)
<pbf> ::= (high)
<pbf> ::= (low)
<pbf> ::= (ema <pbf> <period>)
<period> ::= (shortterm) | (mediumterm) | (longterm)